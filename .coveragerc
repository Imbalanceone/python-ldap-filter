[run]
branch = True
source = ldap_filter/
omit = ldap_filter/parser.py

[report]
ignore_errors = False
precision = 1
exclude_lines =
    pragma: no cover
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    if PY2
    if not PY2

[paths]
source =
    ldap_filter/
    .tox/*/lib/python*/site-packages/